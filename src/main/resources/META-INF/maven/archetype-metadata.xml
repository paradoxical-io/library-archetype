<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="jersey-extras"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/test/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>settings.xml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8" filtered="true">
      <directory></directory>
      <includes>
        <include>.gitignore</include>
        <include>.travis.yml</include>
        <include>deploy.sh</include>
        <include>perform-release.sh</include>
        <include>prep-release.sh</include>
        <include>README.md</include>
        <include>rollback-release.sh</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
